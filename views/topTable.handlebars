<div class="row searchRow">
	<div clas="col-xs-12">
		<h1>What.cd Artist Top</h1>
		<form class="form-inline" method="get" action="/top">
			 <div class="form-group">
    			<input type="text" class="form-control form-control-sm" id="searchInput" placeholder="Artist" name="artist">
  			</div>
  			<button type="submit" class="btn btn-primary btn-sm">Get</button>
		</form>
	</div>
</div>
<div class="row">
	<div clas="col-xs-12 col-md-8 ">
		<div class="table-responsive">
			<table class="table table-striped table-sm">
			  <thead class="thead-inverse">
			    <tr>
			      <th>Snatched</th>
			      <th>Media</th>
			      <th>Album Title</th>
			      <th>Seeders/Leechers</th>
			    </tr>
			  </thead>
			  <tbody>
			  	{{#each data.torrents}}
			    <tr>
			      	<th scope="row">{{this.snatched}}</th>
			      	<td>{{this.media}}</td>
			      	<td><a href="https://what.cd/torrents.php?id={{this.groupId}}&torrentid={{this.id}}#{{this.id}}" target="_blank">{{{this.title}}} <small>[{{this.format}}/{{this.encoding}}]</small></a></td>
		
			      	<td>{{this.seeders}}/{{this.leechers}}</td>
			    </tr>
			    {{/each}} 
			  </tbody>
			</table>
		</div>
	</div>
</div>