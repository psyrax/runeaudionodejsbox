<div class="row" ng-controller="RuneBoxCtrl">
	<div clas="col-xs-12 col-md-8 ">
		<h1>What.cd Artist Top</h1>
		<div class="table-responsive">
			<table class="table table-striped table-sm">
			  <thead class="thead-inverse">
			    <tr>
			    	<th>#</th>
			    	<th>Title</th>
			    	<th>Date</th>
			    </tr>
			  </thead>
			  <tbody>
			    <tr ng-repeat="download in downloads">
			    	<td>\{{ $index + 1 }}</td>
			      	<td><a href="#" ng-click="makeDownload(download)" class="downloadLink">\{{download.name}}</a></td>
			      	<td>\{{download.fromNow}}</td>
			    </tr>
			  </tbody>
			</table>
		</div>
	</div>
</div>
<script type="text/javascript">
	var _downloads = {{{ data.downloads }}};
</script>
